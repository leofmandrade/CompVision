    "lint": "next lint"
  },
  "dependencies": {
    "chart.js": "^4.4.3",
    "next": "^14.2.3",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "pnpm": "^6.0.0"
  }
}
